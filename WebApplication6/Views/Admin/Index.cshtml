@model IEnumerable<AppUser>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <td>
                Id
            </td>
            <td>
                UserName
            </td>
            <td>
                Email
            </td>
        </tr>
        
    </thead>
    <tbody>

        @if (Model.Count() == 0)
        {
            <tr>
                No User Found
            </tr>
        }
        else
        {

            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.UserName</td>
                    <td>@item.Email</td>
                    
                </tr>
            }
        }
        </tbody>
</table>

